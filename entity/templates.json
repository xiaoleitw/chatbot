[
  {
    "template"     : "single",
    "result-type"  : "value",
    "parameters"   : [ "item" ],
    "patterns": [
      [ { "once" : "@{item}" } ]
    ]
  },

  {
    "template"     : "or-list",
    "result-type"  : "list",
    "parameters" : [ "item" ],
    "patterns": [
      [ { "once" : "@{item}" }, { "many" : "，@{item}" }, { "once": "或@{item}" }  ],
      [ { "once" : "@{item}" }, { "many" : "，@{item}" }, { "once": "或者@{item}" } ],
      [ { "once" : "@{item}" }, { "many" : "@{item}" },   { "once": "或@{item}" }   ],
      [ { "once" : "@{item}" }, { "many" : "@{item}" },   { "once": "还是@{item}好呢" }]
    ]
  },

  {
    "template"     : "and-list",
    "result-type"  : "list",
    "parameters"   : [ "item" ],
    "patterns": [
      [ { "once" : "@{item}" }, { "many" : "，@{item}" }, { "once": "和@{item}" }   ],
      [ { "once" : "@{item}" }, { "many" : "，@{item}" }, { "once": "以及@{item}" } ],
      [ { "once" : "@{item}" }, { "many" : "，@{item}" }, { "once": "还有@{item}" } ],
      [ { "once" : "@{item}" }, { "many" : "@{item}" },   { "once": "和@{item}" }   ]
    ]
  },

  {
    "template"     : "centered-range",
    "result-type"  : "value",
    "parameters"   : [ "center" ],
    "patterns": [
      [ { "once" : "@{center}左右" } ],
      [ { "once" : "@{center}前后" } ],
      [ { "once" : "大约@{center}前后" } ],
      [ { "once" : "大概@{center}前后" } ],
      [ { "once" : "大致@{center}左右" } ],
      [ { "once" : "大约@{center}左右" } ]
    ]
  },

  {
    "template"     : "range",
    "result-type"  : "dict",
    "parameters"   : [ "from", "to" ],
    "patterns": [
      [ { "once" : "从@{from}到@{to}" } ],
      [ { "once" : "从@{from}到@{to}之间" } ],
      [ { "once" : "从@{from}至@{to}" } ],
      [ { "once" : "@{from}到@{to}" } ],
      [ { "once" : "@{from}至@{to}" } ],
      [ { "once" : "@{from}到@{to}之间" } ]
    ]
  }
]
