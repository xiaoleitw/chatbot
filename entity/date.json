[
  {
    "entity" : "date",
    "choice" : [
      "single-date",
      "range-date"
    ]
  },

  {
    "entity" : "range-date",
    "compound" : [
      {"name" : "from",  "type": "single-date",  "mandatory" : true},
      {"name" : "to",    "type": "single-date",  "mandatory" : true}
    ],
    "patterns": [
      "@{from}到@{to}",
      "@{from}至@{to}",
      "@{from}到@{to}之间"
    ]
  },
  {
    "entity" : "single-date",
    "choice" : [
      "week-day",
      "relative-day",
      "regular-day"
    ]
  },

  {
    "entity" : "week-day",
    "enum" : {
      "source" : "week-day.csv",
      "column" : 1
    },
    "model":false
  },

  {
    "entity" : "relative-day",
    "enum" : {
      "source" : "relative-day.csv",
      "column" : 1
    },
    "model":false
  },

  {
    "entity" : "regular-day",
    "compound" : [
      {"name" : "month",  "type": "regular-month",  "mandatory" : false},
      {"name" : "day",    "type": "day",            "mandatory" : true}
    ],
    "patterns": [
      "@{month}@{day}",
      "@{month}@{day}左右"
    ]
  },

  {
    "entity" : "regular-month",
    "compound" : [
      {"name" : "year",  "type": "year",  "mandatory" : false},
      {"name" : "month", "type": "month", "mandatory" : true}
    ],
    "patterns": [
      "@{year}@{month}"
    ]
  },

  {
    "entity" : "year",
    "enum" : {
      "source" : "year.csv",
      "column"      : 1
    },
    "model" : false
  },

  {
    "entity" : "month",
    "enum" : {
      "source" : "month.csv",
      "column"      : 1
    },
    "model" : false
  },

  {
    "entity" : "day",
    "enum" : {
      "source" : "day.csv",
      "column"      : 1
    },
    "model":false
  }
]
